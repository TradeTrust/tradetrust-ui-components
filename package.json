{
  "name": "@govtechsg/tradetrust-ui-components",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeTrust/tradetrust-ui-components.git"
  },
  "license": "Apache-2.0",
  "sideEffects": [
    "*.css"
  ],
  "main": "build/index.js",
  "types": "build/esm/index.d.ts",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "build": "NODE_ENV=production CI=false run-s build:*",
    "build:tw-config": "cpy 'src/tailwind.js' build",
    "build:css": "postcss src/tailwind.css -o build/styles.css",
    "build:js": "webpack --mode production",
    "build:type": "tsc --module es6 --declaration --emitDeclarationOnly --outDir build/esm",
    "integration": "testcafe chrome integration/tests/**/*.spec.ts",
    "integration:ci": "start-server-and-test storybook:ci http://localhost:9009 integration:headless",
    "integration:single": "testcafe chrome -L",
    "integration:headless": "testcafe -c 3 chrome:headless integration/tests/**/*.spec.ts",
    "lint": "eslint . --ext .ts,.tsx,.mdx --max-warnings 0",
    "lint:type": "npx --no-install tsc --noEmit --sourceMap false",
    "lint:fix": "npm run lint -- --fix",
    "prep": "run-s lint:fix lint:type test build integration:ci",
    "semantic-release": "semantic-release",
    "start": "run-p start:css storybook",
    "start:css": "postcss src/tailwind.css -o .storybook/styles.css --watch",
    "storybook": "sleep 2 && start-storybook -p 9009",
    "storybook:build": "npm run storybook:css && sleep 2 && build-storybook -o docs",
    "storybook:ci": "npm run storybook:css && sleep 2 && start-storybook -p 9009 --ci",
    "storybook:css": "NODE_ENV=production postcss src/tailwind.css -o .storybook/styles.css",
    "test": "jest --ci",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Opera versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 1 Safari version",
    "IE >= 11"
  ],
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@govtechsg/address-identity-resolver": "^1.4.1",
    "debug": "^4.3.1",
    "react-feather": "^2.0.9",
    "start-server-and-test": "^1.13.1",
    "tailwindcss": "^2.2.17",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.2",
    "@storybook/manager-webpack5": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/debug": "4.1.6",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.13",
    "@types/validator": "^13.6.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-mdx": "^1.13.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.6",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^17.4.4",
    "testcafe": "^1.15.1",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
